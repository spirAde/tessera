{
  "name": "tessera",
  "version": "0.1.0",
  "description": "Tessera - where projects come to life, page by page, component by component",
  "main": "dist/index.js",
  "scripts": {
    "build:watch": "tsc -w",
    "build": "rm -rf dist && tsc",
    "db:create:test": "sequelize db:create",
    "db:drop:test": "sequelize db:drop",
    "db:migrate:new": "npx sequelize migration:generate --name",
    "db:migrate:test": "sequelize db:migrate",
    "start": "node dist/index.js",
    "schema:create": "ts-node scripts/generateJsonSchema.ts",
    "lint": "eslint . --max-warnings 0 --ext .ts,.tsx",
    "lint:fix": "eslint . --max-warnings 0 --ext .ts,.tsx --fix",
    "prettier": "npx prettier --check .",
    "prettier:write": "npx prettier --write .",
    "type-check": "tsc --noEmit --noUnusedLocals --noUnusedParameters",
    "type-coverage": "type-coverage --strict --at-least 97",
    "unused-exports": "ts-prune"
  },
  "author": "Dmitry Spiridonov",
  "license": "UNLICENSED",
  "dependencies": {
    "@fastify/autoload": "^5.8.0",
    "@fastify/static": "^6.12.0",
    "@loadable/component": "^5.15.3",
    "@loadable/server": "^5.16.1",
    "@opentelemetry/api": "^1.4.1",
    "@opentelemetry/auto-instrumentations-node": "^0.40.2",
    "@opentelemetry/core": "^1.19.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.46.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.39.1",
    "@opentelemetry/instrumentation": "^0.39.1",
    "@opentelemetry/propagator-b3": "^1.13.0",
    "@opentelemetry/resources": "^1.19.0",
    "@opentelemetry/sdk-metrics": "^1.19.0",
    "@opentelemetry/sdk-node": "^0.46.0",
    "@opentelemetry/sdk-trace-base": "^1.19.0",
    "@opentelemetry/sdk-trace-node": "^1.19.0",
    "@opentelemetry/semantic-conventions": "^1.19.0",
    "cls-hooked": "^4.2.2",
    "common-tags": "^1.8.2",
    "dotenv": "^16.3.1",
    "fastify": "^4.24.3",
    "fastify-metrics": "^10.5.0",
